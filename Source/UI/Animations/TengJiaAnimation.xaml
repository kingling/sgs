	<local:AnimationBase.Triggers>
		<EventTrigger RoutedEvent="FrameworkElement.Loaded">
			<BeginStoryboard Storyboard="{StaticResource mainAnimation}"/>
		</EventTrigger>
	</local:AnimationBase.Triggers>
	<Grid>
		<local:TengJiaBaseAnimation x:Name="tengJiaBaseAnimation" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="60,50" RenderTransformOrigin="0.5,0.5">
			<local:TengJiaBaseAnimation.RenderTransform>
				<TransformGroup>
					<ScaleTransform/>
					<SkewTransform/>
					<RotateTransform/>
					<TranslateTransform/>
				</TransformGroup>
			</local:TengJiaBaseAnimation.RenderTransform>
			<local:TengJiaBaseAnimation.OpacityMask>
				<RadialGradientBrush>
					<GradientStop Color="#00000000" Offset="0.121"/>
					<GradientStop Color="White" Offset="0.095"/>
				</RadialGradientBrush>
			</local:TengJiaBaseAnimation.OpacityMask>
		</local:TengJiaBaseAnimation>
		<local:BurnAnimation x:Name="burnAnimation" HorizontalAlignment="Stretch" Margin="13,-33,32,57" VerticalAlignment="Stretch" Visibility="Hidden" RenderTransformOrigin="0.5,0.5">
			<local:BurnAnimation.RenderTransform>
				<TransformGroup>
					<ScaleTransform/>
					<SkewTransform/>
					<RotateTransform/>
					<TranslateTransform/>
				</TransformGroup>
			</local:BurnAnimation.RenderTransform>
		</local:BurnAnimation>
		<Image x:Name="image" Source="TengJiaAnimation/hit1.png" Stretch="Fill" Margin="63,54,77,59" Visibility="Hidden">
			<Image.Effect>
				<DropShadowEffect ShadowDepth="1" BlurRadius="10" Color="Red"/>
			</Image.Effect>
		</Image>
		<Image x:Name="image1" Source="TengJiaAnimation/hit2.png" Stretch="Fill" Margin="63,54,77,59" Visibility="Hidden">
			<Image.Effect>
				<DropShadowEffect BlurRadius="10" ShadowDepth="0" Color="Red"/>
			</Image.Effect>
		</Image>
		<Image x:Name="image2" Source="TengJiaAnimation/hit3.png" Stretch="Fill" Margin="63,54,77,59" Visibility="Hidden">
			<Image.Effect>
				<DropShadowEffect BlurRadius="10" Color="Red" ShadowDepth="0"/>
			</Image.Effect>
		</Image></Grid>
